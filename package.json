{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "protobuf2typescript",
  "title": "Protobuf2TypeScript",
  "description": "A script which help front-end devloper to translate the back-end pb file to d.ts in typescript.",
  "icon": "command-icon.png",
  "author": "7gugu",
  "owner": "gustudio",
  "keywords": ["protobuf", "protocol buffer", "typescript"],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Convert ProtoBuf to dTs",
      "subtitle": "PB2TS",
      "description": "Translate protobuf to d.ts",
      "mode": "view"
    },
    {
      "name": "clipboard",
      "title": "Convert ProtoBuf to dTs from clipboard",
      "subtitle": "PB2TS",
      "description": "Translate protobuf to d.ts",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
